{
  "name": "backend",
  "version": "1.0.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "license": "MIT",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "serve:dev": "nodemon dist/server.js",
    "watch": "npm run dirs && concurrently \"npm run build:js --watch\" \"npm run serve:dev\"",
    "dirs": "rimraf dist && mkdirp dist",
    "build": "npm run build:types && npm run build:js",
    "build:types": "npm run tsc",
    "build:js": "babel src --out-dir dist --extensions \".ts\"",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.47",
    "@types/hpp": "^0.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/passport": "^1.0.4",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-github2": "^1.2.4",
    "@types/passport-google-oauth20": "^2.0.4",
    "@types/passport-linkedin-oauth2": "^1.5.1",
    "@types/passport-spotify": "^1.1.0",
    "@types/passport-twitter": "^1.0.36",
    "@types/socket.io": "^2.1.6",
    "@types/web-push": "^3.3.0",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-root-import": "^6.5.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "8.10.1"
  },
  "repository": {
    "url": "https://github.com/halcika7/JobWeb"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.6",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-spotify": "^1.1.0",
    "passport-twitter": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.3.0",
    "tsc": "^1.20150623.0",
    "web-push": "^3.4.4",
    "winston": "^3.2.1",
    "typescript": "^4.0.3"
  }
}
